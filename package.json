{
  "name": "syng",
  "version": "2.0.0",
  "description": "An open source, cross platform, Chinese study app.",
  "main": "app/src/main.js",
  "scripts": {
    "build": "electron-build-env neon build chinese-dictionary --release && rollup -c",
    "dev": "npm run svelte-build && electron . --debug",
    "start": "electron .",
    "svelte-build": "rollup -c",
    "svelte-dev": "rollup -c -w",
    "svelte-start": "sirv app/src",
    "lint": "eslint app/src",
    "fix-lint": "eslint app/src --fix",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sotch-pr35mac/syng"
  },
  "author": "Preston Wang-Stosur-Bassett",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/user-event": "^14.2.0",
    "babel-jest": "^27.3.1",
    "electron": "^12.0.4",
    "electron-build-env": "^0.2.0",
    "electron-reload": "^1.5.0",
    "eslint": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^27.3.1",
    "neon-cli": "^0.7.0",
    "rollup": "^2.0.2",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.49.0",
    "svelte-feather-icons": "^3.2.2",
    "svelte-jest": "^0.3.1",
    "svelte-spa-router": "^2.0.1"
  },
  "dependencies": {
    "chinese-dictionary": "^1.1.1",
    "concurrently": "^6.4.0",
    "elastic-scroll-polyfill": "^2.1.0",
    "hanzi-writer": "^3.3.0",
    "hanzi-writer-data": "^2.0.1",
    "sirv-cli": "^0.4.5",
    "uuid": "^8.3.2"
  }
}
